{
	"compilerOptions": {
		"strict": true,
		"target": "ES2022",
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"moduleResolution": "node",
		"outFile": "../docs/index.js",
		"module": "AMD",
		"baseUrl": ".",
        "incremental": true,
        "noImplicitOverride": true,
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": true,
        "noUnusedLocals": true,
		"skipLibCheck": true,
		"noErrorTruncation": true,
		"paths": {
		}
	}
}